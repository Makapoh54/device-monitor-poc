{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@app/device-client(|/.*)$": "<rootDir>/../../../libs/device-client/src/$1",
    "^@app/common(|/.*)$": "<rootDir>/../../../libs/common/src/$1"
  }
}
