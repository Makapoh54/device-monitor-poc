{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/device-monitor/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/device-monitor/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/device-monitor",
  "projects": {
    "device-client": {
      "type": "library",
      "root": "libs/device-client",
      "entryFile": "index",
      "sourceRoot": "libs/device-client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/device-client/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "device-emulator": {
      "type": "application",
      "root": "apps/device-emulator",
      "entryFile": "main",
      "sourceRoot": "apps/device-emulator/src",
      "compilerOptions": {
        "tsConfigPath": "apps/device-emulator/tsconfig.app.json"
      }
    },
    "device-monitor": {
      "type": "application",
      "root": "apps/device-monitor",
      "entryFile": "main",
      "sourceRoot": "apps/device-monitor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/device-monitor/tsconfig.app.json"
      }
    }
  }
}